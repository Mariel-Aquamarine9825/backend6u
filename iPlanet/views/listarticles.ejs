<div class="container mt-3">
    <h3>My Articles</h3>
    <table class="table table-dark table-hover">
        <thead>
            <tr>
                <th>
                    Date
                </th>
                <th>
                    Title
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% for (let article of articles) { %>
                <tr>
                    <td>
                        <%= article.publishDate %>
                    </td>
                    <td>
                        <%= article.title %>
                    </td>
                    <td>
                        <a href="/editarticle/<%= article.id %>">Edit</a>
                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>
</div>